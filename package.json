  "xo": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "plugins": [
      "better",
      "fp",
      "import",
      "promise",
      "standard",
      "xod-fp"
    ],
    "extends": "standard-pure-fp",
    "rules": {
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "space-before-function-paren": [
        "warn",
        "never"
      ],
      "indent": [
        "warn"
      ],
      "better/no-ifs": [
        "warn"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "import/no-commonjs": [
        "off",
        "allow-primitive-modules"
      ],
      "fp/no-mutation": [
        "error",
        {
          "commonjs": true,
          "allowThis": true
        }
      ]
    }
  },
  "eslintConfig": {
    "parserOptions": {
